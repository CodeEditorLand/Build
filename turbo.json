{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"Document": {
			"dependsOn": ["^Document"],
			"outputs": ["Target/**", "Document/**"]
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["Target/**"]
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^prepublishOnly"]
		},
		"prepublishOnly": {
			"dependsOn": ["^prepublishOnly"],
			"outputs": ["Target/**"]
		},
		"start": {
			"cache": false,
			"dependsOn": ["^prepublishOnly"]
		}
	}
}

{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"Document": {
			"dependsOn": ["^Document"],
			"outputs": ["Documentation/**"]
		},
		"prepublishOnly": {
			"dependsOn": ["^prepublishOnly"],
			"outputs": ["Target/**"]
		},
		"Start": {
			"cache": false,
			"dependsOn": ["^Start"]
		}
	}
}
